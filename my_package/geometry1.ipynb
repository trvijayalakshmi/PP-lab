   "source": [
    "def square(a):\n",
    "    area_s = a*a\n",
    "    peri_s = 4*a\n",
    "    return area_s, peri_s\n",
    "def rectangle(a,b):\n",
    "    area_p = a*b\n",
    "    peri_p = 2*(a+b)\n",
    "    return area_p, peri_p\n",
    "def triangle(a,b,c):\n",
    "    area_t = 0.5 * a * b\n",
    "    peri_t = a + b + c\n",
    "    return area_t, peri_t"
   ]
  }
